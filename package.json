{
  "name": "sailpoint-developer-portal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port=4200",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve --port=4200",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "gen-api-docs": "docusaurus gen-api-docs",
    "clean-api-docs": "docusaurus clean-api-docs",
    "gen-api-docs:version": "docusaurus gen-api-docs:version",
    "clean-api-docs:version": "docusaurus clean-api-docs:version",
    "gen-api-docs-all": "docusaurus gen-api-docs:version isc_versioned:all --plugin-id isc-api && docusaurus gen-api-docs isc_versioned --plugin-id isc-api && docusaurus gen-api-docs iiq --plugin-id iiq-api && docusaurus gen-api-docs nerm --plugin-id nerm-api",
    "clean-api-docs-all": "docusaurus clean-api-docs isc_versioned --plugin-id isc-api && docusaurus clean-api-docs:version isc_versioned:all --plugin-id isc-api && docusaurus clean-api-docs iiq --plugin-id iiq-api && docusaurus clean-api-docs nerm --plugin-id nerm-api",
    "rebuild-docs": "npm run clean-api-docs-all && npm run gen-api-docs-all",
    "speecy-beta-spec": "speccy resolve --quiet static/api-specs/idn/sailpoint-api.beta.yaml -o static/code-examples/beta/beta.yaml",
    "overlay-code-examples-beta": "node src/components/overlay.js static/code-examples/beta/beta.yaml  static/code-examples/beta/code_examples_overlay.yaml",
    "speecy-v3-spec": "speccy resolve --quiet static/api-specs/idn/sailpoint-api.v3.yaml -o static/code-examples/v3/v3.yaml",
    "overlay-code-examples-v3": "node src/components/overlay.js static/code-examples/v3/v3.yaml  static/code-examples/v3/code_examples_overlay.yaml",
    "speecy-v2024-spec": "speccy resolve --quiet static/api-specs/idn/sailpoint-api.v2024.yaml -o static/code-examples/v2024/v2024.yaml",
    "overlay-code-examples-v2024": "node src/components/overlay.js static/code-examples/v2024/v2024.yaml  static/code-examples/v2024/code_examples_overlay.yaml",
    "gen-all-api-specs-code-examples": "npm run speecy-beta-spec && npm run overlay-code-examples-beta && npm run speecy-v3-spec && npm run overlay-code-examples-v3 && npm run speecy-v2024-spec && npm run overlay-code-examples-v2024"
  },
  "dependencies": {
    "@docusaurus/faster": "^3.7.0",
    "@docusaurus/plugin-client-redirects": "3.7.0",
    "@docusaurus/plugin-google-tag-manager": "3.7.0",
    "@docusaurus/theme-mermaid": "3.7.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@gracefullight/docusaurus-plugin-microsoft-clarity": "^1.0.0",
    "@mdx-js/react": "^3.0.0",
    "@mui/material": "^6.1.6",
    "@typeform/embed-react": "^1.21.0",
    "ace-builds": "^1.36.4",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "docusaurus-plugin-openapi-docs": "4.3.1",
    "docusaurus-theme-openapi-docs": "4.3.1",
    "js-yaml": "^4.1.0",
    "jsonpathly": "github:sailpoint-oss/jsonpathly#v2.2.0",
    "ldrs": "^1.0.1",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.2.0",
    "react-ace": "^13.0.0",
    "react-dom": "^18.2.0",
    "react-live": "^4.0.0",
    "react-markdown": "^8.0.7",
    "react-spinners": "^0.13.8",
    "react-tabs": "^4.3.0",
    "speccy": "^0.8.7"
  },
  "overrides": {
    "mermaid": "10.9.3"
  },
  "devDependencies": {
    "@docusaurus/core": "3.7.0",
    "@docusaurus/module-type-aliases": "3.7.0",
    "@docusaurus/preset-classic": "3.7.0",
    "@docusaurus/types": "3.7.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.25.0",
    "husky": "^8.0.2",
    "prettier": "2.8.0",
    "pretty-quick": "^3.1.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
